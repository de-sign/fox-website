<nav class="fw-nav uk-width-expand uk-padding uk-overflow-auto">
    {% if FW.menu %}
        <ul class="uk-nav {{ FW.template.nav.class }}"
            {% if FW.template.nav.scrollspy %}
                uk-scrollspy-nav="{{ FW.template.nav.scrollspy }}"
            {% endif %}
        >
            {% for fw_menu in FW.menu %}
                <li class="uk-nav-header">{{ fw_menu.label | safe }}</li>
                <li class="uk-nav-divider"></li>
                {% for fw_sub in fw_menu.sub %}
                    <li {% if fw_sub.sub %} class="uk-parent" {% endif %} >
                        <a href="{{ fw_sub.href | replace( FW.url.path, "" ) }}">{{ fw_sub.label | safe }}</a>
                        {% if fw_sub.sub %}
                            <ul class="uk-nav-sub">
                                {% for fw_ssub in fw_sub.sub %}
                                    <li>
                                        <a href="{{ fw_ssub.href | replace(FW.url.path, '') }}">{{ fw_ssub.label | safe }}</a>
                                    </li>
                                {% endfor %}
                            </ul>
                        {% endif %}
                    </li>
                {% endfor %}
            {% endfor %}
        </ul>
    {% endif %}
</nav>