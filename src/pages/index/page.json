{
    "url": {
        "path": "/index.html"
    },

    "template": {
        "name": "background",
        "navbar": null,
        "mode": "uk-light",
        "background": {
            "type": "image",
            "attributes": {
                "data-src": "{{ FW.paths.images }}/backgrounds/leaf-dark.jpg",
                "class": "uk-background-cover"
            }
        }
    },

    "content": {
        "icon": [
            {
                "type": "uk-svg",
                "attributes": {
                    "data-src": "{{ FW.paths.images }}/icons/foxwebsite.svg",
                    "width": 200,
                    "height": 200,
                    "alt": "Fox Website logo",
                    "class": "uk-text-emphasis uk-visible@s"
                }
            },
            {
                "type": "uk-svg",
                "attributes": {
                    "data-src": "{{ FW.paths.images }}/icons/foxwebsite.svg",
                    "width": 150,
                    "height": 150,
                    "alt": "Fox Website logo",
                    "class": "uk-text-emphasis uk-hidden@s"
                }
            }
        ],
        "title": "FOX Website",
        "subtitle": "Simple Node.js project for develop<br/>your static website",
        "description": null,
        "footer": [
            {
                "type": "uk-button",
                "tag": "a",
                "content": "Get started",
                "attributes": {
                    "href": "{{ FW.paths.html }}/doc.html",
                    "class": "uk-button-primary uk-margin-small-bottom uk-margin-small-left uk-margin-small-right uk-width-2-3 uk-width-auto@s"
                }
            },
            {
                "type": "uk-button",
                "tag": "a",
                "attributes": {
                    "href": "https://github.com/de-sign/fox-website",
                    "class": "uk-button-default uk-margin-small-bottom uk-margin-small-left uk-margin-small-right uk-width-2-3 uk-width-auto@s",
                    "target": "_blank",
                    "rel": "noreferrer noopener"
                },
                "content": "<span uk-icon=\"github\"></span> Github"
            }
        ]
    }
}